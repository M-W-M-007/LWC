<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Prevent_Delete_City</fullName>
    <active>true</active>
    <description>Prevents deletion of cities.</description>
    <errorConditionFormula>Number_of_Cities__c &lt;  PRIORVALUE(Number_of_Cities__c)</errorConditionFormula>
    <errorMessage>Deletion of cities is not allowed.</errorMessage>
</ValidationRule>
