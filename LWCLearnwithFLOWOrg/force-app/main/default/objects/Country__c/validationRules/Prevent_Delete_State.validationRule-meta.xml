<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Prevent_Delete_State</fullName>
    <active>true</active>
    <description>Prevents deletion of states.</description>
    <errorConditionFormula>Number_of_States__c &lt;  PRIORVALUE(Number_of_States__c)</errorConditionFormula>
    <errorMessage>Deletion of states or provinces is not allowed.</errorMessage>
</ValidationRule>
