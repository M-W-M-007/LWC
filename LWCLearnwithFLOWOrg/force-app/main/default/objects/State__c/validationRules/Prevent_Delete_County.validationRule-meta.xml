<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Prevent_Delete_County</fullName>
    <active>true</active>
    <description>Prevents deletion of counties.</description>
    <errorConditionFormula>Number_of_Counties__c &lt;  PRIORVALUE(Number_of_Counties__c)</errorConditionFormula>
    <errorMessage>Deletion of counties is not allowed.</errorMessage>
</ValidationRule>
